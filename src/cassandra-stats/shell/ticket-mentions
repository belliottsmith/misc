grep -E "><time class='livestamp' datetime='|https://issues.apache.org/jira/secure/ViewProfile\.jspa\?name=[^\"]+\"" issues.apache.org/jira/browse/CASSANDRA-$1 | grep -E "><time class='livestamp' datetime='$2" -A 1 | grep -E "https://issues.apache.org/jira/secure/ViewProfile\.jspa\?name=[^\"]+\"" | sed -r 's@.*/jira/secure/ViewProfile\.jspa\?name=([^"]*)".*@\1@g' | sort | uniq -c
